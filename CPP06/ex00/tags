CXX	Makefile	/^CXX = c++ -fsanitize=address -ggdb$/;"	m
NAME	Makefile	/^NAME = Conversor$/;"	m
OBJS	Makefile	/^OBJS = ${SRCS:%.cpp=${OBJS_FOLDER}\/%.o}$/;"	m
OBJS_FOLDER	Makefile	/^OBJS_FOLDER = objs$/;"	m
RM	Makefile	/^RM = rm -rf$/;"	m
SRCS	Makefile	/^SRCS =	main.cpp ScalarConverter.cpp$/;"	m
ScalarConverter	ScalarConverter.cpp	/^ScalarConverter::ScalarConverter() {}$/;"	f	class:ScalarConverter
