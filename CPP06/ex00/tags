ScalarConverter	ScalarConverter.hpp	/^class ScalarConverter$/;"	c
VPATH	Makefile	/^VPATH = srcs\/$/;"	m
all	Makefile	/^all : 		${NAME}$/;"	t
clean	Makefile	/^clean:$/;"	t
convert	ScalarConverter.cpp	/^static void	ScalarConverter::convert(const std::string str)$/;"	f	class:ScalarConverter	typeref:typename:void	file:
fclean	Makefile	/^fclean: 	clean$/;"	t
isChar	ScalarConverter.cpp	/^bool	ScalarConverter::isChar(const std::string &str)$/;"	f	class:ScalarConverter	typeref:typename:bool
isPseudo	ScalarConverter.cpp	/^bool	ScalarConverter::isPseudo(const std::string &str)$/;"	f	class:ScalarConverter	typeref:typename:bool
main	main.cpp	/^int main(int argc, char **argv)$/;"	f	typeref:typename:int
operator =	ScalarConverter.cpp	/^ScalarConverter	&ScalarConverter::operator=(const ScalarConverter& other)$/;"	f	class:ScalarConverter	typeref:typename:ScalarConverter &
re	Makefile	/^re: 		fclean all$/;"	t
~ScalarConverter	ScalarConverter.cpp	/^ScalarConverter::~ScalarConverter() {}$/;"	f	class:ScalarConverter
